  "description": "Sistema automatizado de trading CoinbitClub com validação automática de contas",
  "main": "coinbitclub-launcher.js",
  "scripts": {
    "start": "node coinbitclub-launcher.js",
    "dev": "nodemon coinbitclub-launcher.js",
    "validate": "node sistema-validacao-automatica.js",
    "test-connections": "node teste-trade-real.js",
    "check-balances": "node buscar-saldos-completo.js",
    "quick-diagnostic": "node diagnostico-rapido.js",
    "setup-database": "node setup-database.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop coinbitclub-bot",
